      <uri>model://gedik_platform</uri>
      <pose>3 0 0.1 0 0 0</pose>
    </include>

    <model name="iris">
      <include>
        <uri>model://drone_with_camera</uri>
      </include>
      <pose> 0 0 0 0 0 0</pose>
    </model>
